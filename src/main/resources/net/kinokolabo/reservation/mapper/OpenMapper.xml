<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.kinokolabo.reservation.mapper.OpenMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO OPEN (START_DATETIME, END_DATETIME) VALUES (#{startDatetime}, #{endDateTime})
    </insert>
    <update id="update">
        UPDATE OPEN SET START_DATETIME　=　#{startDatetime}, END_DATETIME = #{endDateTime}, WHERE ID=#{id}
    </update>
    <select id="selectAll" resultType="net.kinokolabo.reservation.domain.Open">
        SELECT * FROM OPEN
    </select>
</mapper>